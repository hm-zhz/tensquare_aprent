<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >

	<!--1.修改为自己的mysql连接jar包:绝对路径地址；-->
	<classPathEntry location="C:\\ziliao\\10-15-maven\\repository\\mysql\\mysql-connector-java\\5.1.20\\mysql-connector-java-5.1.20.jar"/>

	<context id="context1" >
		<commentGenerator  >
			<property name="javaFileEncoding" value="UTF-8"/>
			<property name="suppressAllComments" value="false" />
			<property name="suppressDate" value="true"/>
		</commentGenerator>
		<!--2.选择数据库的driverClass与connectionURL-->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
		connectionURL="jdbc:mysql://localhost:3306/java1302?useUnicode=true&amp;useSSL=false&amp;characterEncoding=UTF-8" userId="root" password="root" />

		<!--3.生成实体类 指定包名 以及生成的绝对地址 （可以自定义地址，但是路径不存在会自动创建 ） -->
		<javaModelGenerator targetPackage="com.sz.tencent.model" targetProject="C:\Users\Administrator.USER-20190902XF\IdeaProjects\spring_boot1028\src\main\java" >
			<property name="enableSubPackages" value="true"/> <!-- 如果true，MBG会根据catalog和schema来生成子包。如果false就会直接用targetPackage属性。默认为false -->
			<property name="trimStrings" value="true" /> <!-- 当数据库字段使用CHAR时，建议在<javaModelGenerator>中设 -->
		</javaModelGenerator>

		<!--4.生成Mapper映射文件地 址 -->
		<sqlMapGenerator targetPackage="com.sz.tencent.mapper"  targetProject="C:\Users\Administrator.USER-20190902XF\IdeaProjects\spring_boot1028\src\main\resources" />

		<!--5.生成Mapper 接口文件 可以配置 type="XMLMAPPER"生成xml的dao实现 -->
		<javaClientGenerator targetPackage="com.sz.tencent.mapper" targetProject="C:\Users\Administrator.USER-20190902XF\IdeaProjects\spring_boot1028\src\main\java" type="XMLMAPPER" />

		<!--6.配置哪些 表需要自动生成-->
		<table tableName="chn_city" enableCountByExample="true" enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true"   selectByExampleQueryId="true" />
		<table tableName="chn_province" enableCountByExample="true" enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true"   selectByExampleQueryId="true" />
	</context>
</generatorConfiguration>